A,OAAA,EAAA;AACA;AACA,IAAA,cAAA,EAAA,0BAAA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,0CAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,cADA;AAEA,QAAA,KAAA,EAAA,KAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,MAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,OANA;AAOA,KAVA;;AAWA;AACA,IAAA,WAAA,EAAA,uBAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,gCAAA,EACA,IADA,CACA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,IAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,cAAA;AACA,OAJA;AAKA,KAlBA;;AAmBA;AACA,IAAA,mBAAA,EAAA,6BAAA,MAAA,EAAA,QAAA,EAAA;AAAA;;AACA,WAAA,QAAA,CAAA,MAAA,GAAA,MAAA;AACA,WAAA,QAAA,CAAA,QAAA,GAAA,QAAA;AACA,WAAA,QAAA,CAAA,OAAA,GAAA,KAAA,cAAA,CAHA,CAIA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,uCAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,QAAA,CAAA,OADA;AAEA,QAAA,EAAA,EAAA,KAAA,QAAA,CAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,CAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,cAAA,CAAA,cAAA,GAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,cAAA;AACA,OAPA,EALA,CAaA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,iCAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,QAAA,CAAA,OADA;AAEA,QAAA,EAAA,EAAA,KAAA,QAAA,CAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,MAAA,EAAA;AACA,QAAA,MAAA,CAAA,gBAAA,GAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,OANA;AAOA,WAAA,OAAA,GAAA,CAAA,KAAA,OAAA;AACA;AA1CA,GArCA;AAiFA,EAAA,OAjFA,qBAiFA;AACA;AACA,SAAA,WAAA;AACA;AApFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"conts-area\">\r\n    <div class=\"conts-tit\">\r\n      <h2>년도별 월별 사용자별 휴가 사용 현황</h2>\r\n    </div>\r\n    <div class=\"list-top-area\">\r\n      <label for=\"s1\">년도</label>\r\n      <select id=\"s1\" title=\"select\" v-model=\"sel_searchYear\" v-on:change=\"loadDayOffList()\">\r\n        <option v-for=\"v in searchYear\" v-bind:key=\"v.year\" v-bind:value=\"v.year\">{{ v.year }}</option>\r\n      </select>\r\n      <label for=\"s2\">월</label>\r\n      <select id=\"s2\" title=\"select\" v-model=\"sel_searchMonth\" v-on:change=\"loadDayOffList()\">\r\n        <option>01</option>\r\n        <option>02</option>\r\n        <option>03</option>\r\n        <option>04</option>\r\n        <option>05</option>\r\n        <option>06</option>\r\n        <option>07</option>\r\n        <option>08</option>\r\n        <option>09</option>\r\n        <option>10</option>\r\n        <option>11</option>\r\n        <option>12</option>\r\n      </select>\r\n    </div>\r\n    <div class=\"tbl-area\">\r\n      <!-- table detail start -->\r\n      <table class=\"tbl tb-detail\" id=\"tableId\">\r\n        <caption>디테일 테이블</caption>\r\n        <colgroup>\r\n          <col style=\"width:30%\">\r\n          <col style=\"width:70%\">\r\n        </colgroup>\r\n        <thead>\r\n          <tr>\r\n            <th>성명</th>\r\n            <th>휴가사용일수/발생일수</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr class=\"select\" v-for=\"v in dayOffList\" v-bind:key=\"v.id\">\r\n            <th v-on:click=\"userVacationByMonth(v.id, v.name)\" style=\"cursor: pointer\">{{ v.name }}</th>\r\n            <td>{{ v.sumMinusDayoff }} / {{ v.acnt }}</td>\r\n          </tr>\r\n        </tbody>\r\n      <userVacationByMonthPop :visible.sync=\"visible\" :tempData.sync=\"tempData\" :eachDayOffList.sync=\"eachDayOffList\" :eachMonthOffList.sync=\"eachMonthOffList\">\r\n      </userVacationByMonthPop>\r\n      </table>\r\n      <!-- table detail end -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport moment from 'moment'\r\nimport userVacationByMonthPop from '../gostop/userVacationByMonthPop.vue'\r\n\r\nexport default {\r\n  name: 'DeptPlando',\r\n  data () {\r\n    return {\r\n      searchYear: [],\r\n      sel_searchYear: moment(new Date()).format('yyyy'),\r\n      sel_searchMonth: moment(new Date()).format('MM'),\r\n      visible: false,\r\n      tempData: {\r\n        userId: '',\r\n        userName: '',\r\n        selYear: ''\r\n      },\r\n      dayOffList: {\r\n        eachMember: {\r\n          id: '',\r\n          name: '',\r\n          acnt: '',\r\n          sumMinusDayoff: ''\r\n        }\r\n      },\r\n      eachDayOffList: {\r\n        acnt: '',\r\n        sumMinusDayoff: ''\r\n      },\r\n      eachMonthOffList: [\r\n        {\r\n          months: '',\r\n          dayOff: '',\r\n          halfOff: ''\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  components: {\r\n    userVacationByMonthPop: userVacationByMonthPop\r\n  },\r\n  methods: {\r\n    /* 연도별, 사용자별 휴가 사용 현황 리스트 */\r\n    loadDayOffList: function () {\r\n      axios.post('/api/vacation/selectAllMembersDayOffList', {\r\n        gdate: this.sel_searchYear,\r\n        month: this.sel_searchMonth\r\n      })\r\n        .then((result) => {\r\n          this.dayOffList = result.data.data\r\n        })\r\n    },\r\n    /* 근태 접수된 연도 */\r\n    loadAllYear: function () {\r\n      axios.get('/api/vacation/selectGuntaeYear')\r\n        .then((result) => {\r\n          this.searchYear = result.data.data\r\n          this.loadDayOffList()\r\n        })\r\n    },\r\n    /* 각 사용자 이름 클릭 액션 ==> 월별,연도별 사용자 휴가사용 현황 팝업 open */\r\n    userVacationByMonth: function (userId, userName) {\r\n      this.tempData.userId = userId\r\n      this.tempData.userName = userName\r\n      this.tempData.selYear = this.sel_searchYear\r\n      // 연도별 휴가사용일수 / 발생일수\r\n      axios.post('/api/vacation/selectMembersDayOffList', {\r\n        gdate: this.tempData.selYear,\r\n        id: this.tempData.userId\r\n      })\r\n        .then((result) => {\r\n          this.eachDayOffList.acnt = result.data.data.acnt\r\n          this.eachDayOffList.sumMinusDayoff = result.data.data.sumMinusDayoff\r\n        })\r\n      // 월별 휴가사용 일자\r\n      axios.post('/api/vacation/selectUsersOffDay', {\r\n        gdate: this.tempData.selYear,\r\n        id: this.tempData.userId\r\n      })\r\n        .then((result) => {\r\n          this.eachMonthOffList = result.data.data\r\n        })\r\n      this.visible = !this.visible\r\n    }\r\n  },\r\n  created () {\r\n    /* 페이지 로드시 기본작동 메소드 */\r\n    this.loadAllYear()\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/gostop"}]}