AA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA;AACA,OAXA,EAWA,KAXA,CAWA,YAAA;AACA,QAAA,KAAA,CAAA,+BAAA,CAAA;AACA,OAbA;AAcA,KA5BA;AA6BA,IAAA,cAAA,EAAA,0BAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,4BAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,YADA;AAEA,QAAA,OAAA,EAAA,KAAA,MAAA,CAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,OANA,EAMA,KANA,CAMA,YAAA;AACA,QAAA,KAAA,CAAA,+BAAA,CAAA;AACA,OARA;AASA,KAvCA;AAwCA,IAAA,QAAA,EAAA,oBAAA;AACA,WAAA,cAAA;AACA,KA1CA;AA2CA,IAAA,OAAA,EAAA,mBAAA;AACA,UAAA,KAAA,YAAA,KAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,eAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,cAAA;AACA;AACA;AAjDA,GAPA;AA0DA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,cAAA;AACA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"conts-area\">\r\n    <div class=\"conts-tit\">\r\n      <h2>템플릿 관리</h2>\r\n    </div>\r\n    <div class=\"list-top-area\">\r\n      <div class=\"fl wp30\">\r\n        <label for=\"s1\">구분</label>\r\n        <select id=\"s1\" title=\"구분\" class=\"wp50\" v-model=\"listSelected\" @change=\"onChange\">\r\n          <option value=\"0\">선택</option>\r\n          <option v-for=\"(item, i) in listCode\" v-bind:key=\"i\" v-bind:value=\"item.scode\">{{ item.scodeNm }}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"fr\">\r\n        <button type=\"button\" class=\"btn btn-blue\" @click=\"onClick\">저장</button>\r\n      </div>\r\n    </div>\r\n    <div class=\"tbl-area\">\r\n      <!-- table list start -->\r\n      <table class=\"tbl\">\r\n        <caption>템플릿 관리</caption>\r\n        <colgroup>\r\n          <col style=\"width:20%\">\r\n          <col style=\"width:80%\">\r\n        </colgroup>\r\n        <tbody>\r\n          <tr>\r\n            <th>내용</th>\r\n            <td><textarea id=\"ta1\" name=\"ta1\" v-model=\"detail.content\" style=\"height:200px\"></textarea></td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <!-- //table list end -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'Template',\r\n  data: () => ({\r\n    listSelected: '0',\r\n    listCode: [],\r\n    detail: {}\r\n  }),\r\n  methods: {\r\n    selectListCode: function () {\r\n      axios.get('/api/mypage/codeList', {\r\n        params: {\r\n          grcode: 'A012'\r\n        }\r\n      })\r\n        .then(res => {\r\n          this.listCode = res.data.data\r\n        }).catch(() => {\r\n          alert('오류가 발생하였습니다. 시스템 관리자에게 문의하세요.')\r\n        })\r\n    },\r\n    selectTemplate: function () {\r\n      axios.get('/api/mypage/templateDetail', {\r\n        params: {\r\n          gubun: this.listSelected\r\n        }\r\n      })\r\n        .then(res => {\r\n          if (res.data.data != null) {\r\n            this.detail = res.data.data\r\n          } else {\r\n            this.detail.content = ''\r\n          }\r\n        }).catch(() => {\r\n          alert('오류가 발생하였습니다. 시스템 관리자에게 문의하세요.')\r\n        })\r\n    },\r\n    updateTemplate: function () {\r\n      axios.post('/api/mypage/templateUpdate', {\r\n        gubun: this.listSelected,\r\n        message: this.detail.content\r\n      })\r\n        .then(res => {\r\n          alert(res.data.message)\r\n        }).catch(() => {\r\n          alert('오류가 발생하였습니다. 시스템 관리자에게 문의하세요.')\r\n        })\r\n    },\r\n    onChange: function () {\r\n      this.selectTemplate()\r\n    },\r\n    onClick: function () {\r\n      if (this.listSelected === '0') {\r\n        alert('구분을 선택하여 주세요.')\r\n      } else {\r\n        this.updateTemplate()\r\n      }\r\n    }\r\n  },\r\n  created: function () {\r\n    this.selectListCode()\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/vcode"}]}