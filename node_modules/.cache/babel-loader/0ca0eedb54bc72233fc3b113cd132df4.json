cAAA,EAAA,0BAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,sBAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,MAAA,EAAA;AADA;AADA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA,KA5BA;AA6BA,IAAA,gBAAA,EAAA,4BAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,sBAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,MAAA,EAAA;AADA;AADA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA,OARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAVA;AAWA,KAzCA;AA0CA,IAAA,qBAAA,EAAA,iCAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,6BAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,QAAA,EAAA,KAAA,QADA;AAEA,UAAA,OAAA,EAAA,KAAA;AAFA;AADA,OAAA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAVA;AAWA,KAtDA;AAuDA,IAAA,uBAAA,EAAA,iCAAA,MAAA,EAAA;AAAA;;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,+BAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,MAAA,EAAA;AADA;AADA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,OARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAVA;AAWA,KAnEA;AAoEA,IAAA,iBAAA,EAAA,6BAAA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,+BAAA,EAAA;AACA,QAAA,MAAA,EAAA,KAAA,MAAA,CAAA,MADA;AAEA,QAAA,OAAA,EAAA,KAAA,cAFA;AAGA,QAAA,UAAA,EAAA,KAAA,OAHA;AAIA,QAAA,KAAA,EAAA,KAAA,MAAA,CAAA,KAJA;AAKA,QAAA,KAAA,EAAA,KAAA,MAAA,CAAA,KALA;AAMA,QAAA,KAAA,EAAA,KAAA,MAAA,CAAA,KANA;AAOA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA,QAPA;AAQA,QAAA,UAAA,EAAA,KAAA,MAAA,CAAA,UARA;AASA,QAAA,SAAA,EAAA,KAAA,UAAA,CAAA,QAAA,CAAA,KAAA,cAAA,CAAA,GAAA,CAAA,EAAA;AATA,OAAA,EAWA,IAXA,CAWA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OAfA,EAeA,KAfA,CAeA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAjBA;AAkBA,KAvFA;AAwFA,IAAA,cAAA,EAAA,0BAAA;AACA,WAAA,qBAAA;AACA;AA1FA,GAjBA;AA6GA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,cAAA;AACA,SAAA,gBAAA;AACA,SAAA,qBAAA;AACA;AAjHA,CAAA","sourcesContent":["<template>\r\n  <div class=\"conts-area\">\r\n    <div class=\"conts-tit\">\r\n      <h2>결재수신함</h2>\r\n    </div>\r\n    <div class=\"list-top-area\">\r\n      <div class=\"t-right\">\r\n        <label for=\"s1\">결재상태</label>\r\n        <select id=\"s1\" title=\"select\" class=\"wp20\" v-model=\"listSelected\" @change=\"onSelectChange()\">\r\n          <option value=\"0\">전체</option>\r\n          <option v-for=\"(item, i) in listCode\" v-bind:key=\"i\" v-bind:value=\"item.scode\">{{ item.scodeNm }}</option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n    <div class=\"tbl-area\">\r\n\r\n      <!-- table list start -->\r\n      <table class=\"tbl\">\r\n        <caption>리스트</caption>\r\n        <colgroup>\r\n          <col style=\"width:10%\">\r\n          <col style=\"width:10%\">\r\n          <col style=\"width:50%\">\r\n          <col style=\"width:10%\">\r\n          <col style=\"width:10%\">\r\n          <col style=\"width:10%\">\r\n        </colgroup>\r\n        <thead>\r\n          <tr>\r\n            <th>No</th>\r\n            <th>결재종류</th>\r\n            <th>결재사유</th>\r\n            <th>상신자</th>\r\n            <th>결재상태</th>\r\n            <th>결재</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-if=\"list.length == 0\">\r\n            <td colspan=\"6\">\r\n              <div class=\"t-center pd50\">결재내용이 없습니다.</div>\r\n            </td>\r\n          </tr>\r\n          <tr v-for=\"(item, i) in list\" v-bind:key=\"i\" v-else>\r\n            <td>{{ item.serial }}</td>\r\n            <td>{{ item.appGubunNm }}</td>\r\n            <td class=\"t-left\">{{ item.vreason }}</td>\r\n            <td>{{ item.name }}</td>\r\n            <td>\r\n              <div>{{ item.appCodeNm }}</div>\r\n            </td>\r\n            <td><a href=\"#\" v-on:click=\"openModal(item.serial)\" v-if=\"item.appCode == '1'\">결재</a></td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <!-- //table list end -->\r\n    </div>\r\n    <!-- Modal start -->\r\n    <MyModal v-if=\"modal\">\r\n      <template slot=\"header\">\r\n        결재\r\n      </template>\r\n      <!-- default 슬롯 콘텐츠 -->\r\n      <div class=\"tbl-area\">\r\n        <table class=\"tbl tb-detail mt10\">\r\n          <caption>디테일 테이블</caption>\r\n          <colgroup>\r\n            <col style=\"width:30%\">\r\n            <col style=\"width:70%\">\r\n          </colgroup>\r\n          <tbody>\r\n            <tr>\r\n              <th>결재선택</th>\r\n              <td class=\"t-left\">\r\n                <label for=\"s1\" class=\"blind\">결재</label>\r\n                <select id=\"s1\" title=\"select\" class=\"wp100\" v-model=\"detailSelected\">\r\n                  <option v-for=\"(item, i) in detailCode\" v-bind:key=\"i\" v-bind:value=\"item.scode\">{{ item.scodeNm }}</option>\r\n                </select>\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <th>휴가일자</th>\r\n              <td class=\"t-left\">\r\n                <span>{{ detail.sdate }}</span> ~ <span>{{ detail.edate }}</span>\r\n              </td>\r\n            </tr>\r\n            <tr>\r\n              <th>결재종류</th>\r\n              <td class=\"t-left\"><span>{{ detail.appGubunNm }} ( {{ detail.vcodeNm }} )</span></td>\r\n            </tr>\r\n            <tr>\r\n              <th>결재내용</th>\r\n              <td><textarea id=\"ta1\" name=\"ta1\" v-model=\"message\"></textarea></td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <!-- /default -->\r\n      <!-- footer 슬롯 콘텐츠 -->\r\n      <template slot=\"footer\">\r\n        <div class=\"t-center\">\r\n          <button type=\"button\" class=\"btn btn-blue\" v-on:click=\"doSend\">저장</button>\r\n          <button type=\"button\" class=\"btn btn-gray\" v-on:click=\"closeModal\">취소</button>\r\n        </div>\r\n      </template>\r\n      <!-- /footer -->\r\n    </MyModal>\r\n    <!-- modal end -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyModal from '../../components/MyModal.vue'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'AppReceive',\r\n  components: { MyModal },\r\n  data () {\r\n    return {\r\n      listCode: [],\r\n      detailCode: [],\r\n      modal: false,\r\n      message: '',\r\n      appGubun: '1',\r\n      appStatus: '',\r\n      listSelected: '0',\r\n      detailSelected: '2',\r\n      list: [],\r\n      detail: {}\r\n    }\r\n  },\r\n  methods: {\r\n    openModal (serial) {\r\n      this.selectApprovalRecDetail(serial)\r\n    },\r\n    closeModal () {\r\n      this.modal = false\r\n      this.detailSelected = '2'\r\n      this.message = ''\r\n      this.selectApprovalRecList()\r\n    },\r\n    doSend () {\r\n      if (this.message.length > 0) {\r\n        this.updateApprovalRec()\r\n      } else {\r\n        alert('결재내용을 입력하여 주세요.')\r\n      }\r\n    },\r\n    selectListCode: function () {\r\n      axios.get('/api/mypage/codeList', {\r\n        params: {\r\n          grcode: 'A010'\r\n        }\r\n      })\r\n        .then(res => {\r\n          this.listCode = res.data.data\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    selectDetailCode: function () {\r\n      axios.get('/api/mypage/codeList', {\r\n        params: {\r\n          grcode: 'A010'\r\n        }\r\n      })\r\n        .then(res => {\r\n          this.detailCode = res.data.data\r\n          this.detailCode.splice(0, 1)\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    selectApprovalRecList: function () {\r\n      axios.get('/api/mypage/approvalRecList', {\r\n        params: {\r\n          appGubun: this.appGubun,\r\n          appCode: this.listSelected\r\n        }\r\n      })\r\n        .then(res => {\r\n          this.list = res.data.data\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    selectApprovalRecDetail: function (serial) {\r\n      axios.get('/api/mypage/approvalRecDetail', {\r\n        params: {\r\n          serial: serial\r\n        }\r\n      })\r\n        .then(res => {\r\n          this.detail = res.data.data\r\n          this.modal = true\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    updateApprovalRec: function () {\r\n      axios.post('/api/mypage/approvalRecUpdate', {\r\n        serial: this.detail.serial,\r\n        appCode: this.detailSelected,\r\n        appContent: this.message,\r\n        sdate: this.detail.sdate,\r\n        edate: this.detail.edate,\r\n        vcode: this.detail.vcode,\r\n        submitId: this.detail.submitId,\r\n        appGubunNm: this.detail.appGubunNm,\r\n        appStatus: this.detailCode[parseInt(this.detailSelected) - 2].scodeNm\r\n      })\r\n        .then(res => {\r\n          alert(res.data.message)\r\n          this.message = ''\r\n          this.closeModal()\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    onSelectChange: function () {\r\n      this.selectApprovalRecList()\r\n    }\r\n  },\r\n  created: function () {\r\n    this.selectListCode()\r\n    this.selectDetailCode()\r\n    this.selectApprovalRecList()\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/mypage"}]}