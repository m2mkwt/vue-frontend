me=\"grCode\" v-on:dblclick=\"dblGrCode(i, value)\" > {{ i.grCode }} </td>\r\n                <!-- 그룹코드명 -->\r\n                <td class=\"t-left\" name=\"search\" id=\"search\" v-on:click=\"clickGrCodeNm(value)\" v-html=\"value.grCodeNm\" v-on:dblclick=\"dblGrCode(i, value)\">\r\n                  <span class=\"cursor\">\r\n                    {{ i.grCodeNm }}\r\n                  </span>\r\n                </td>\r\n                <!-- 사용여부 -->\r\n                <!-- v-bind:checked=\"value.guseYn=='Y'\"-->\r\n                <td v-on:dblclick=\"dblGrCode(i, value)\" id=\"guseYn\" name=\"guseYn\">\r\n                  <!-- <label for=\"boxList\" class=\"blind\"></label> -->\r\n                    <input readonly type=\"checkbox\" id=\"chkbox\" name=\"chkbox\" v-bind:checked=\"value.guseYn=='Y'\" true-value=\"Y\" false-value=\"N\" v-on:click=\"ifChecked()\" v-html=\"value.guseYn\" value=\"value.guseYn\" />\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <!-- table list end -->\r\n        </div>\r\n      </div>\r\n      <div class=\"fl wp49\">\r\n        <p class=\"mt30 mb10\">세부목록</p>\r\n        <div class=\"tbl-area\">\r\n          <!-- table list start -->\r\n          <table class=\"tbl\">\r\n            <caption>list 테이블</caption>\r\n            <colgroup>\r\n              <col style=\"width:20%\">\r\n              <col style=\"width:40%\">\r\n              <col style=\"width:15%\">\r\n              <col style=\"width:15%\">\r\n              <col style=\"width:10%\">\r\n            </colgroup>\r\n            <thead>\r\n              <tr>\r\n                <th>세부코드</th>\r\n                <th>세부코드명</th>\r\n                <th>사용여부</th>\r\n                <th>우선순위</th>\r\n                <th>비고</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr>\r\n                <td colspan=\"5\" v-show=\"isHidden\">코드명을 선택해 주세요.</td>\r\n              </tr>\r\n              <tr v-for=\"(value, i) in searchList\" v-bind:key=\"i\"  v-on:dblclick=\"dblSCode(i, value)\">\r\n                <!-- 세부 코드 -->\r\n                <td v-html=\"value.scode\" name=\"scode\" value=\"i.scode\">{{ i.scode }}\r\n                </td>\r\n                <!-- 세부 코드명 -->\r\n                <td class=\"t-left\" v-html=\"value.scodeNm\" name=\"scodeNm\">{{ i.scodeNm }} </td>\r\n                <!-- 사용여부 -->\r\n                <td value=\"i.suseYn\">\r\n                  <label for=\"chk3\" class=\"blind\">체크박스</label> {{ i.suseYn }}\r\n                  <input type=\"checkbox\" id=\"chk3\" name=\"chk3\" v-html=\"value.suseYn\" v-bind:checked=\"value.suseYn=='Y'\" true-value=\"Y\" false-value=\"N\" v-on:click=\"ifChecked()\">\r\n                </td>\r\n                <!-- 순서 -->\r\n                <td v-html=\"value.priority\" name=\"priority\">{{ i.priority }}</td>\r\n                <!-- 비고 -->\r\n                <td v-html=\"value.bigo1\" name=\"bigo1\">{{ i.bigo1 }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <!-- table list end -->\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'CommonCode',\r\n  data: function () {\r\n    return {\r\n      poJson: [],\r\n      modal: false,\r\n      grCode: [],\r\n      grCodeNm: [],\r\n      guseYn: [],\r\n      selectAll: [],\r\n      searchList: [],\r\n      scode: [],\r\n      scodeNm: [],\r\n      priority: [],\r\n      suseYn: [],\r\n      bigo1: [],\r\n      bigo2: '',\r\n      bigo3: '',\r\n      search: '',\r\n      isHidden: true,\r\n      src: 'http://localhost:3000/vcode/common',\r\n      checked: false,\r\n      btnHidden: true,\r\n      btnHidden2: true,\r\n      btnSave: false,\r\n      btnSave2: false,\r\n      trHidden: true,\r\n      select: true,\r\n      edit: true,\r\n      selectedRow: '',\r\n      selectedSRow: ''\r\n    }\r\n  },\r\n  created: function () {\r\n    this.firstSelect()\r\n  },\r\n  methods: {\r\n    // 처음 그룹코드 전체조회\r\n    firstSelect: function () {\r\n      console.log('grCode는 ==>' + this.grCode)\r\n      axios.get('/api/commonCode/selectCommonCodeList')\r\n        .then(res => {\r\n          console.log(res.data)\r\n          this.selectAll = res.data.data\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    // 체크박스 클릭\r\n    ifChecked: function () {\r\n      // 체크 안되어있는 상태가 true\r\n      if (this.checked === false) {\r\n        this.checked = true\r\n        this.guseYn = 'Y'\r\n        this.suseYn = 'Y'\r\n      } else {\r\n        this.checked = false\r\n        this.guseYn = 'N'\r\n        this.suseYn = 'N'\r\n      }\r\n    },\r\n    // 더블 클릭시 수정 - 세부코드\r\n    dblSCode: function (i, value) {\r\n      // 신규 로우 추가 됐을 때 동작 안하게\r\n      if (this.btnSave === false && this.btnSave2 === false) {\r\n      // 중복 클릭 방지\r\n        if (this.edit === true && i !== this.selectedSRow) {\r\n          this.btnHidden2 = false\r\n          // 비고가 null일 경우 빈칸 처리\r\n          if (value.bigo1 === null) {\r\n            value.bigo1 = ''\r\n          }\r\n          this.$set(this.searchList, i, {\r\n            scode: '<input type=\"text\" name=\"sodeInput\" value=\"' + value.scode + '\" readonly>',\r\n            scodeNm: '<input type=\"text\" name=\"scodeNmInput\" value=\"' + value.scodeNm + '\">',\r\n            suseYn: '<input type=\"text\" name=\"suseYnInput\" v-bind:checked=\"value.suseYn==' + 'Y' + '\" value=\"' + value.suseYn + '\">',\r\n            priority: '<input type=\"text\" name=\"priorityInput\" value=\"' + value.priority + '\">',\r\n            bigo1: '<input type=\"text\" name=\"bigo1Input\" value=\"' + value.bigo1 + '\">'\r\n          })\r\n          this.selectedSRow = i\r\n        } else {\r\n          alert('이미 선택한 행입니다.')\r\n        }\r\n      } else {\r\n        alert('신규 추가 중 수정 불가합니다.')\r\n      }\r\n    },\r\n    // 세부코드 수정 저장\r\n    updateSCode: function () {\r\n      var scode = document.getElementsByName('sodeInput')\r\n      var scodeNm = document.getElementsByName('scodeNmInput')\r\n      var suseYn = this.suseYn\r\n      var priority = parseInt(document.getElementsByName('priorityInput'))\r\n      var bigo1 = document.getElementsByName('bigo1Input')\r\n      console.log('scode :: ', scode[0])\r\n      for (var i = 0; i < scodeNm.length; i++) {\r\n        if (suseYn[i] == null) {\r\n          suseYn[i] = 'N'\r\n        }\r\n        var scodeList = {\r\n          grCode: this.grCode,\r\n          scode: scode[i].value,\r\n          scodeNm: scodeNm[i].value,\r\n          suseYn: suseYn[i],\r\n          priority: priority[i],\r\n          bigo1: bigo1[i].value\r\n        }\r\n      }\r\n      this.poJson.push(scodeList)\r\n      axios.post('/api/commonCode/updateSCode', this.poJson)\r\n        .then(res => {\r\n          console.log(res.data.data)\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n      window.location.reload()\r\n    },\r\n    // 그룹코드 수정 저장\r\n    updateCommonCode: function () {\r\n      var grCode = document.getElementsByName('grCodeInput')\r\n      var grCodeNm = document.getElementsByName('grCodeNmInput')\r\n      var guseYn = this.guseYn\r\n      for (var i = 0; i < grCodeNm.length; i++) {\r\n        if (guseYn[i] == null) {\r\n          guseYn[i] = 'N'\r\n        }\r\n        var grCodeList = { grCode: grCode[i].value, grCodeNm: grCodeNm[i].value, guseYn: guseYn[i] }\r\n        console.log('왓???????????' + grCodeList)\r\n      }\r\n      this.poJson.push(grCodeList)\r\n      axios.post('/api/commonCode/updateCommonCode', this.poJson)\r\n        .then(res => {\r\n          console.log(res.data.data)\r\n          // this.selectAll = res.data.data\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n      window.location.reload()\r\n    },\r\n    // 그룹 신규 취소\r\n    cancelGrCode: function () {\r\n      if (confirm('정말 취소하시겠습니까??') === true) {\r\n        window.location.reload()\r\n      } else {\r\n        return false\r\n      }\r\n    },\r\n    // 더블 클릭시 수정 - 그룹코드\r\n    dblGrCode: function (i, value) {\r\n      console.log(this.btnSave)\r\n      // 신규 로우 추가 됐을 때 동작 안하게\r\n      if (this.btnSave === false && this.btnSave2 === false) {\r\n      // 중복 클릭 방지\r\n        if (this.edit === true && i !== this.selectedRow) {\r\n          this.btnHidden = false\r\n          this.$set(this.selectAll, i, {\r\n            grCode: '<input type=\"text\" name=\"grCodeInput\" value=\"' + value.grCode + '\" readonly>',\r\n            grCodeNm: '<input type=\"text\" name=\"grCodeNmInput\" value=\"' + value.grCodeNm + '\">',\r\n            guseYn: '<input type=\"text\" name=\"guseYn\" v-bind:checked=\"' + value.guseYn + '==' + 'Y' + '\" value=\"' + value.guseYn + '\">'\r\n          }\r\n          )\r\n          this.selectedRow = i\r\n        } else {\r\n          alert('이미 선택한 행입니다.')\r\n        }\r\n      } else {\r\n        alert('신규 입력 중 수정 불가합니다.')\r\n      }\r\n    },\r\n    // 세부 목록 신규 버튼 클릭\r\n    addScode: function () {\r\n      console.log(this.grCode)\r\n      if (this.grCode.length !== 0) {\r\n        this.btnSave2 = true\r\n        this.searchList.push({\r\n          scode: '<input type=\"text\" name=\"scodeInput\">',\r\n          scodeNm: '<input type=\"text\" name=\"scodeNmInput\">',\r\n          suseYn: '<input type=\"radio\" value=\"Y\" name=\"suseYnInput' + this.searchList.length + '\"' + 'checked=\"checked\" checked=\"checked\" /> Y <input type=\"radio\" value=\"N\" name=\"suseYnInput' + this.searchList.length + '\"' + 'checked=\"checked\"/> N',\r\n          priority: '<input type=\"text\" name=\"priorityInput\">',\r\n          bigo1: '<input type=\"text\" name=\"bigo1Input\">'\r\n        })\r\n      } else {\r\n        alert('그룹 코드를 먼저 선택해 주세요.')\r\n      }\r\n    },\r\n    // 세부 코드 저장\r\n    insertScode: function () {\r\n      var scode = document.getElementsByName('scodeInput')\r\n      var scodeNm = document.getElementsByName('scodeNmInput')\r\n      var suseYn = this.suseYn\r\n      var priority = document.getElementsByName('priorityInput')\r\n      var bigo1 = document.getElementsByName('bigo1Input')\r\n      for (var i = 0; i < scode.length; i++) {\r\n        if (suseYn[i] == null) {\r\n          suseYn[i] = 'N'\r\n        }\r\n        var sCodeList = {\r\n          grCode: this.grCode,\r\n          scode: scode[i].value,\r\n          scodeNm: scodeNm[i].value,\r\n          suseYn: suseYn[i],\r\n          priority: priority[i].value,\r\n          bigo1: bigo1[i].value\r\n        }\r\n        this.poJson.push(sCodeList)\r\n      }\r\n      axios.post('/api/commonCode/insertSCode', this.poJson)\r\n        .then(res => {\r\n          window.location.reload()\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    // 그룹 코드 신규 버튼 클릭\r\n    addGrCode: function () {\r\n      this.btnSave = true\r\n      this.selectAll.push({\r\n        grCode: '<input type=\"text\" name=\"grCodeInput\">',\r\n        grCodeNm: '<input type=\"text\" name=\"grCodeNmInput\" id=\"grCodeNmInput\">',\r\n        guseYn: '<input type=\"checkbox\" v-model=\"checked\" v-on:click=\"ifChecked()\">'\r\n      })\r\n    },\r\n    // 그룹 코드 저장\r\n    insertCommonCode: function () {\r\n      var grCode = document.getElementsByName('grCodeInput')\r\n      var grCodeNm = document.getElementsByName('grCodeNmInput')\r\n      var guseYn = this.guseYn\r\n      for (var i = 0; i < grCode.length; i++) {\r\n        if (guseYn[i] == null) {\r\n          guseYn[i] = 'N'\r\n        }\r\n        if (grCode[i].value !== '' || grCodeNm[i].value !== '') {\r\n          var grCodeList = {\r\n            grCode: grCode[i].value,\r\n            grCodeNm: grCodeNm[i].value,\r\n            guseYn: guseYn[i]\r\n          }\r\n          this.poJson.push(grCodeList)\r\n          axios.post('/api/commonCode/insertCommonCode', this.poJson)\r\n            .then(res => {\r\n              window.location.reload()\r\n            }).catch((error) => {\r\n              console.log(error)\r\n            })\r\n        }\r\n      }\r\n    },\r\n    // 검색\r\n    searchCommonCode: function () {\r\n      this.isHidden = false\r\n      axios.post('/api/commonCode/searchCommonCode', {\r\n        grCodeNm: this.grCodeNm\r\n      })\r\n        .then(res => {\r\n          console.log('널일 땐?', res.data.data)\r\n          this.searchList = res.data.data\r\n          if (this.searchList.length === 0 || this.grCodeNm === '') {\r\n            alert('그룹코드명을 입력해주세요')\r\n          }\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    // 그룹 코드명 클릭\r\n    clickGrCodeNm: function (value) {\r\n      // 이미 선택한 채로 선택할 때 세부 신규 버튼 안바뀌게\r\n      this.isHidden = false\r\n      this.grCode = value.grCode\r\n      axios.post('/api/commonCode/searchCommonCode', {\r\n        grCodeNm: value.grCodeNm\r\n      })\r\n        .then(res => {\r\n          console.log(res.data.data)\r\n          this.searchList = res.data.data\r\n        }).catch((error) => {\r\n          console.log(error)\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/views/vcode"}]}